{
  "name": "app",
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets",
    "build:watch": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=public/assets --loader:.js=jsx --watch",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "axios": "^1.12.2",
    "bootstrap": "^5.3.8",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "web-vitals": "^5.1.0"
  }
}
